{
  "name": "wedding-site",
  "dependencies": {
    "browserify": "^9.0.4",
    "browserify-incremental": "^1.5.0",
    "browserify-shim": "^3.8.3",
    "classnames": "^1.2.0",
    "coffee-react": "^3.0.1",
    "coffee-reactify": "^3.0.0",
    "coffee-script": "*",
    "coffeeify": "^1.0.0",
    "del": "^1.1.1",
    "font-awesome": "^4.3.0",
    "gulp": "^3.8.11",
    "gulp-debug": "^2.0.1",
    "gulp-rev": "^3.0.1",
    "gulp-rev-collector": "^0.1.4",
    "gulp-autoprefixer": "^2.1.0",
    "gulp-changed": "^1.2.1",
    "gulp-notify": "^2.2.0",
    "gulp-sass": "^1.3.3",
    "gulp-sequence": "^0.3.2",
    "gulp-sourcemaps": "^1.5.1",
    "gulp-util": "^3.0.4",
    "gulp-watch": "^4.2.3",
    "jest-cli": "^0.4.0",
    "jquery": "*",
    "lodash": "^3.6.0",
    "moment": "^2.9.0",
    "pretty-hrtime": "^1.0.0",
    "react": "^0.13",
    "react-tools": "^0.13",
    "reactify": "^1.1.0",
    "require-dir": "^0.3.0",
    "through2": "^0.6.3",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.1.0"
  },
  "browserify": {
    "transform": [
      "coffee-reactify",
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "jquery": "$"
  },
  "scripts": {
    "postinstall": "gulp build",
    "test": "jest",
    "test-debug": "node-debug --nodejs --harmony node_modules/.bin/jest --runInBand"
  },
  "jest": {
    "rootDir": "./gulp/assets/javascripts/components",
    "moduleFileExtensions": [
      "js",
      "cjsx"
    ],
    "scriptPreprocessor": "<rootDir>/__tests__/preprocessor.js",
    "testFileExtensions": [
      "cjsx"
    ],
    "unmockedModulePathPatterns": [
      "react",
      "jquery",
      "classnames",
      "moment"
    ],
    "testPathIgnorePatterns": [
      "preprocessor.js"
    ]
  },
  "license": "MIT",
  "engines": {
    "node": ">= 0.10"
  }
}
